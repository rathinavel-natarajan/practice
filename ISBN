    {
        scanf("%c ",&a[i]);
        printf("%c",a[i]);
    }
for(i=0;i<10;i++)
{
    if(strcmp(a[i],u)==0)
    {
       for(u=0;u<10;u++)
       {
        if(j<10)
        {
            x=10;
            l=0;
            b[0+l]=a[j]*x;
            l++;
            x--;
        }
        for(k=0;k<10;k++)
        {
            sum=sum+b[k];
        }
          if(sum%11==0)
            printf("%d",u);
            else
                j++;
       }
    }
        //else
        //    printf("INVALID INPUT");

}
